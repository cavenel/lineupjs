{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "target": "es6",
    "outDir": "build"
  },
  "typedocOptions": {
    "name": "LineUpJS",
    "hideGenerator": true,
    "exclude": "**/*.(d.ts|js|spec.ts|scss)",
    "excludePrivate": true,
    "excludeProtected": true,
    "excludeExternals": true,
    "listInvalidSymbolLinks": true,
    "plugin": ["typedoc-plugin-as-member-of"]
  }
}
